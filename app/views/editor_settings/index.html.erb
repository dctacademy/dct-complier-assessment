<p id="notice"><%= notice %></p>

<h1>Editor Settings</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Theme name</th>
      <th>Theme css</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @editor_settings.each do |editor_setting| %>
      <tr>
        <td><%= editor_setting.user_id %></td>
        <td><%= editor_setting.theme_name %></td>
        <td><%= editor_setting.theme_css %></td>
        <td><%= link_to 'Show', editor_setting %></td>
        <td><%= link_to 'Edit', edit_editor_setting_path(editor_setting) %></td>
        <td><%= link_to 'Destroy', editor_setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Editor Setting', new_editor_setting_path %>
